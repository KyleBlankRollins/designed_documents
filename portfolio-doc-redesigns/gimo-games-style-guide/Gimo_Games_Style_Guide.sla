        <PAGEOBJECT XPOS="215.333333333333" YPOS="741.6" OwnPage="0" ItemID="122978272" PTYPE="4" WIDTH="95.3333333333333" HEIGHT="50.4" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="1" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L95.3333 0 L95.3333 50.4 L0 50.4 L0 0 Z" copath="M0 0 L95.3333 0 L95.3333 50.4 L0 50.4 L0 0 Z" gXpos="215.333333333333" gYpos="741.6" gWidth="0" gHeight="0" PSTYLE="Body" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Body" LINESP="4" FONT="Raleway Medium" FCOLOR="White"/>
                <ITEXT FONT="Raleway Medium" FCOLOR="White" CH="Gimo Games"/>
                <para PARENT="Body" LINESP="4"/>
                <ITEXT FONT="Raleway Medium" FCOLOR="White" CH="Kyle Rollins"/>
